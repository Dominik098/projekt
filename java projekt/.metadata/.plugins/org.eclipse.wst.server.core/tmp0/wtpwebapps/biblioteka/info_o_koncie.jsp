        <p>Rola: <%=session.getAttribute("administrator") %></p>
        <p>Email: <%=session.getAttribute("email") %></p>
        <p>Hasło: <%=session.getAttribute("haslo") %></p>
        
      </div>
    </div>
  </div>
</body>
</html>
